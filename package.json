    "@babel/preset-env": "^7.7.7",
    "@svgr/rollup": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.1.0",
